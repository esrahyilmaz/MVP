<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>standard</theme>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>

        <!-- title of the document -->
        <documentTitle>SHC Report</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>Self Healing Code Report</reportName>

        <!-- view report without internet -->
        <offlineMode>false</offlineMode>

        <!-- custom javascript -->
        <js></js>

        <!-- custom styles -->
        <css>
            .seleniumError{
                background-color:rgba(242, 185, 185);
                font-size:11px;
                height: 100px
            }
            .m{
                display: block;
                margin-top: 5px;
                margin-left: 25px;
            }
            .error {
                color: orangered;
            }
            .success {
                color: green;
                font-weight: bold;
            }
        </css>
    </configuration>
</extentreports>